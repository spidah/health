    <tr>
      <td><%= user.id %></td>
      <td><%= h(user.loginname) %></td>
      <td><%= h(user.email) %></td>
      <td><%= user.admin ? 'Yes' : 'No' %></td>
      <td><%= user.gender == 'm' ? 'Male' : 'Female' %></td>
      <td><%= format_date(user.dob) %></td>
      <td><%= format_date(user.last_login) %></td>
      <td class="edit"><%= link_to(image_tag('/images/edit.gif'), edit_admin_user_url(user)) %></td>
      <td class="delete"><%= link_to(image_tag('/images/delete.gif'), destroy_admin_user_url(user), :class => 'delete') %></td>
    </tr>